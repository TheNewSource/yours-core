  datt: Ember.inject.service(),
  fetchTransactionData: function() {this.get('datt').getTransactions(this.get('user')).then(function(transactions) {
    this.set('model', transactions);}.bind(this));
  }.on('init')

<table class='table'>
  <thead>
    <tr>
      <th>Date</th>
      <th>Description</th>
      <th>User</th>
      <th>Amount</th>
    </tr>
  </thead><tbody>

    {{#each model as |transaction|}}
      <tr>
        <td>{{date}}</td>
        <td>{{description}}</td>
        <td>{{user}}</td>
        <td>{{amount}}</td>
      </tr>
    {{/each}}

  </tbody>
</table>

