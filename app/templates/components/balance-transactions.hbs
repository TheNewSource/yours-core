  <thead>
    <tr>
      <th>Date</th>
      <th>Description</th>
      <th>User</th>
      <th>Amount</th>
    </tr>
  </thead><tbody>

    {{#each transactions as |transaction|}}
      <tr>
        <td>{{transaction.date}}</td>
        <td>{{transaction.description}}</td>
        <td>{{transaction.user}}</td>
        <td>{{transaction.amount}}</td>
      </tr>
    {{/each}}

  </tbody>

